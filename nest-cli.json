{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/job-executor/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/job-executor/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/job-executor",
  "projects": {
    "job-executor": {
      "type": "application",
      "root": "apps/job-executor",
      "entryFile": "main",
      "sourceRoot": "apps/job-executor/src",
      "compilerOptions": {
        "tsConfigPath": "apps/job-executor/tsconfig.app.json"
      }
    },
    "job-processor": {
      "type": "application",
      "root": "apps/job-processor",
      "entryFile": "main",
      "sourceRoot": "apps/job-processor/src",
      "compilerOptions": {
        "webpack": false,
        "tsConfigPath": "apps/job-processor/tsconfig.app.json"
      }
    },
    "job-producer": {
      "type": "application",
      "root": "apps/job-producer",
      "entryFile": "main",
      "sourceRoot": "apps/job-producer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/job-producer/tsconfig.app.json"
      }
    }
  }
}